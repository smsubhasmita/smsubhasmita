//SCHOOL DETAILS
const getSchoolDetails=(element)=>{
    var opt =element.options[element.selectedIndex];
    $('#school_name').val(opt.dataset.school);
    $('#block').val(opt.dataset.block);
    $('#ProjectCoordinator').val(opt.dataset.cordinate);

    addAttribute(['#school_name','#block','#ProjectCoordinator']);
}
function addAttribute(elementArray){
    for(var i=0; i < elementArray.length; i++){
        $(elementArray[i]).attr('readonly',true);
    }
}

//*************Total No Of Visit******************/ 

$(".calculate").each(function() {
    $(this).keyup(function(){
        calculateSum();
    });
});

function calculateSum() {
    var sum = 0;
     $(".calculate").each(function() { 
         if(!isNaN(this.value) && this.value.length!=0) {
            sum += parseInt(this.value);
        }
        //.toFixed(2)
    });
     $("#sum").html(sum);
}

//equipment Downtime Detail 2.2
//TABLE SHOW HIDE
const workConditionCheck =(val)=>{
    if(parseInt(val) === 2){
        $('.worKingCondition_yes').show();
    }else{
        $('.worKingCondition_yes').hide(); 
    }
}
//Is equipment Downtime School Working Days
var mynum = 1;
const addDowntimeDetailsRow=()=>{
    mynum ++;
    addDowntimeHtml(mynum);
}

function addDowntimeHtml(count){ //console.log(count);
    var html=`<tr> 
            <td> 
                <div class="form-group"> 
                    <select class="form-control  form-control-sm equipment " name="ewc_matid[]"  required>
                        <option value="">Select Equipment</option> 
                        <?php foreach($Equpement_name as $data): ?>
                        <option value="<?= $data->material_id;?>">
                        <?= $data->material_name; ?>
                        </option>
                        <?php endforeach;?>
                    </select>
                    <div class="help-block with-errors  "></div>
                    <?php echo form_error('ewc_matid[]'); ?>  
                </div>
            </td>
            <td> 
                <div class="form-group">
                    <input type="number" class="form-control form-control-sm working_day" name="ewc_downworkday[]" placeholder="Total No. of downtime in  Working days " min="0" max="31" value="<?= set_value('ewc_downworkday[]');  ?>" onkeyup="addComplainDate(this.value, $(this).data('row'))"  data-row="${count}" required >
                    <div class="help-block with-errors  "></div>
                </div>
            </td> 
            <td>
                <div class="form-group">
                    <select class="form-control form-control-sm ewc_isworking" name="ewc_isworking[]">
                        <option value="">Select Is working?</option>  
                        <option value="1">Yes</option>
                        <option value="2">No</option>
                    </select>
                    <div class="text-danger ewc_isworking_error  "></div>
                </div> 
            </td>   
            <td>  
                <div class="complaint_loged_start${count}"></div> 
            </td> 
            <td>  
                <div class="complaint_closed_close${count}"></div> 
            </td>
            <td>
                <div class="col-sm-2">
                    <i class="fa fa-times data text-danger fa-lg" onClick="$(this).closest('tr').remove()" data-toggle="tooltip" data-placement="top" title="Remove Row" ></i>
                </div>
            </td>
        </tr>`;
    $('.myDowntimeTable').append(html); 
}

const addComplainDate =(value, row)=>{
    var LoggedDate=`<div class="form-group my-1">
                            <input class="form-control form-control-sm complaint_loged" type="date"  id="logged"  min="0" 
                            name="ewd_logdate${row}[]" value="<?= set_value('ewd_logdate[]');?>" required>
                            <div class="help-block with-errors  "></div>
                        </div>`;
    var ClosureDate=`<div class="form-group my-1">
                        <input class="form-control form-control-sm complaint_closed" type="date"  id="Closure" name="ewd_closedate${row}[]" value="<?= set_value('ewd_closedate[]');?>" required>
                        <div class="help-block with-errors  "></div>
                    </div>`; 
    var Logged = LoggedDate.repeat(value);
    $(`.complaint_loged_start${row}`).html(Logged); 
        
    var closerDate=ClosureDate.repeat(value);
    $(`.complaint_closed_close${row}`).html(closerDate); 
}



///TABLE SHOW HIDE
$(document).ready(function(){
   
   //Internet Connectivity Available 4.8 
   $('.internetconnectivityn').on('change', function(){
        var internetconnectivityn = this.checked ? this.value : '';
       
        if(internetconnectivityn=1){
            $('.internetconnectivityn_yes').show();
        }else{
            $('.internetconnectivityn_yes').hide(); 
        }
    });
    $('.internetconnectivityy').on('change', function(){
        var internetconnectivityy = this.checked ? this.value : '';
       
        if(internetconnectivityy=1){
            $('.internetconnectivityn_yes').hide();
        }else{
            $('.internetconnectivityn_yes').show(); 
        }
    });
    //equipment Replacement 5
    $('.replacementEquipment').on('change', function(){
        var replacementEquipment = this.checked ? this.value : '';
         if(replacementEquipment){
            $('.replacementEquipment_Yes').show();
        }else{
            $('.replacementEquipment_Yes').hide(); 
        }
    });
    $('.replacementEquipmentNo').on('change', function(){
        var replacementEquipmentNo = this.checked ? this.value : '';
         if(replacementEquipmentNo){
            $('.replacementEquipment_Yes').hide();
         }
    });
    //Any equipment theft/damage
    $('.equipmentThefDamage').on('change',function(){
        var equipmentThefDamage = this.checked ? this.value : '';
        console.log(equipmentThefDamage);
        if(equipmentThefDamage){
            $('.equipmentThefDamage_list').show();
        }else{
            $('.equipmentThefDamage_list').hide(); 
        }
    });
    $('.equipmentThefDamage_no').on('change',function(){
        var equipmentThefDamage_no = this.checked ? this.value : '';
        console.log(equipmentThefDamage_no);
        if(equipmentThefDamage_no){
            $('.equipmentThefDamage_list').hide();
        
        }
    });
});
 
$(document).ready(function(){
    //Equipment replacement details
    $("#equipment_replacement_add").click(function(){
         var htmlData2=`<tr> 
                            <td>
                                <div class="form-group row"> 
                                    <div class="col-sm-12">
                                        <select class="form-control  form-control-sm equipment_name" name="red_matid[]" id="equipment" required >
