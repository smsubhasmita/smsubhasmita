                                            <?php foreach($Equpement_name as $data): ?>
                                                <option value="<?= $data->material_id;?>"<?= (set_value('red_matid[]')==$data->material_id)?'Selected':'';?>><?= $data->material_name; ?></option>
                                                <?php endforeach;?>
                                        </select>
                                    </div>  
                                </div>
                            </td>
                            <td>
                                <div class="form-group row "> 
                                    <div class="col-sm-11 ">
                                        <input type="text" name="red_desc[]" class="form-control form-control-sm reason_replacement " id="inputPassword" placeholder="Enter Name of the Equipment"  name="red_desc[]" value="<?php set_value('red_desc[]')?>" required> 
                                    </div>  
                                     <button class=" btn  btn-light  btn-xs   float_remove" onClick="$(this).closest('tr').remove()"> <i class="fa  fa-times  data fa-lg p-2 text-danger"></i></button>
                                 
                                </div>
                            </td>
                             
                        </tr> `;
         $('#equipment_replacement > tbody:last').append(htmlData2); 
    }); 
     
    
    // Theft/Damaged Equipment
    $("#theft_damaged_equpment").click(function(){
        var htmlTheftDamage=`<tr>
                                <td>
                                    <div class="form-group  row ">
                                        <div class="col-sm-12 ">
